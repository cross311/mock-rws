      &lt;mdsol:Query CancelURI=&quot;<%= query_edit_url(":query_uuid","Cancel") %>&quot; 
      	CancelVERB=&quot;POST&quot; /&gt;

    &lt;/mdsol:PartialClinicalData&gt;
&lt;/ODM&gt;
		</pre>
	</div>
	</div>


